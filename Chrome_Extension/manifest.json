{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "background": {
      "scripts": ["background.js", "jquery-3.4.1.min.js", "content.js"],
      "persistent": true
    },
    "permissions": [
      "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "commands": {
      "check-codesmell": {
          "suggested_key": {
              "default": "Alt+Shift+L"
          },
          "description": "Load a random link"
      }
    }
}